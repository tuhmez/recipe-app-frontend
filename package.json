    "@types/node": "^14.14.35",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "dotenv-cli": "^5.0.0",
    "jshint": "^2.12.0",
    "material-table": "^1.69.3",
