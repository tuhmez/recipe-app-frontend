  "scripts": {
    "start": "react-scripts start",
    "start:prod": "serve -s build --ssl-cert ./build/file.crt --ssl-key ./build/file.pem -l 443",
    "build": "react-scripts build && cp ./public/file.pem build/ && cp ./public/file.crt build/",
    "build:windows": "react-scripts build && cp ./public/file.pem build/ && cp ./public/file.crt build/ && cp ./Web.config build/",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-links": "^6.1.21",
